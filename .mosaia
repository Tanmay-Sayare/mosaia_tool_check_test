{
    "name": "RugGuardianTest",
    "description": "All in tool for rug checking platform and trader trusted api's manager.",
    "schema": {
        "type": "function",
        "function": {
            "name": "RugChecker",
            "description": "RugChecker is a risk analysis assistant for blockchain projects. It provides insights into the legitimacy of DeFi, NFT, and crypto initiatives.",
            "strict": true,
            "parameters": {
                "type": "object",
                "properties": {
                    "action": {
                        "type": "string",
                        "description": "The RugCheck API action to perform",
                        "enum": [
                            "get_verified_stats",
                            "get_new_tokens",
                            "get_trending_tokens",
                            "get_recent_tokens",
                            "get_token_summary",
                            "get_token_votes"
                        ]
                    },
                    "token_id": {
                        "type": "string",
                        "description": "The token ID required for token_summary and token_votes actions"
                    },
                    "page": {
                        "type": "integer",
                        "description": "Page number for paginated results",
                        "minimum": 1
                    },
                    "limit": {
                        "type": "integer",
                        "description": "Number of results per page",
                        "minimum": 1,
                        "maximum": 100
                    }
                },
                "required": ["action"],
                "additionalProperties": false
            }
        }
    },
    "envVars": [
        "RUGCHECK_BASE_URL"
    ]
}